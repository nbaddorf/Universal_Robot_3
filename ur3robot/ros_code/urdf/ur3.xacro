<?xml version="1.0" ?>
<robot name="ur3" xmlns:xacro="http://www.ros.org/wiki/xacro">

<link name="base_link">
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://ur3master/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver_default"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://ur3master/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="tower">
  <visual>
    <origin xyz="-0.105418 -0.06341 -0.487908" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://ur3master/meshes/tower.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver_default"/>
  </visual>
  <collision>
    <origin xyz="-0.105418 -0.06341 -0.487908" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://ur3master/meshes/tower.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="arm1">
  <visual>
    <origin xyz="-0.143418 -0.06341 -0.829408" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://ur3master/meshes/arm1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver_default"/>
  </visual>
  <collision>
    <origin xyz="-0.143418 -0.06341 -0.829408" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://ur3master/meshes/arm1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="arm2">
  <visual>
    <origin xyz="-0.331194 -0.06341 -0.872808" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://ur3master/meshes/arm2.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver_default"/>
  </visual>
  <collision>
    <origin xyz="-0.331194 -0.06341 -0.872808" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://ur3master/meshes/arm2.stl" scale="0.001 0.001 0.001"/>
