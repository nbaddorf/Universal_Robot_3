<?xml version="1.0"?>
	<robot name="ur3`">

		<link name="base_link">
		</link>
		
		<link name="laser">
		</link>
                
        <link name="webcam">
		</link>

		<link name="imu">
		</link>
                

		<joint name="base_link_to_laser" type="fixed">
			<parent link="base_link"/>
			<child link="laser"/>
			<origin xyz="0 0 0.3429" rpy="0.0 0.0 1.570796"/>
			<axis xyz="0 0 0"/>
		</joint>

        <joint name="base_link_to_webcam" type="fixed">
			<parent link="base_link"/>
			<child link="webcam"/>
			<origin xyz="0.21 -0.051 0.27" rpy="0.0 0.0 0"/>
			<axis xyz="0 0 0"/>
		</joint>	

		<joint name="base_link_to_imu" type="fixed">
			<parent link="base_link"/>
			<child link="imu"/>
			<origin xyz="0.198 -0.153 0.458" rpy="0.0 0.0 1.5708"/>
			<axis xyz="0 0 0"/>
		</joint>	
		
	</robot>
